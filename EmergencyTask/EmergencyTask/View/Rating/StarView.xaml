<?xml version="1.0" encoding="UTF-8"?>
<FFImageLoading:CachedImage xmlns="http://xamarin.com/schemas/2014/forms" 
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       x:Class="EmergencyTask.Views.Rating.StarView"
       xmlns:Images="clr-namespace:EmergencyTask.Views.Rating.Images"
       xmlns:FFImageLoading="clr-namespace:FFImageLoading.Forms;assembly=FFImageLoading.Forms"
       Aspect="AspectFit" WidthRequest="{Binding StarWidth}" HeightRequest="{Binding StarHeight}" VerticalOptions="Center" HorizontalOptions="Center">
    <FFImageLoading:CachedImage.Behaviors>
        <Images:ImageAsset Asset="{Binding Asset}" />
    </FFImageLoading:CachedImage.Behaviors>
</FFImageLoading:CachedImage>